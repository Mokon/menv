<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">

  <title>dash</title>
  <meta name="description" content="dash - A Dashboard Application">
  <meta name="author" content="David Bond">

  <script src="http://code.highcharts.com/highcharts.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>

  <script defer src="https://use.fontawesome.com/releases/v5.0.8/js/all.js"></script>

  <link href="https://cdn.fancygrid.com/fancy.min.css" rel="stylesheet">
  <script src="https://cdn.fancygrid.com/fancy.min.js"></script>

  <link rel="stylesheet/less" type="text/css" href="nav.less" />
  <script src="//cdnjs.cloudflare.com/ajax/libs/less.js/3.0.0/less.min.js" ></script>

  <script>
  Fancy.MODULESDIR="https://cdn.fancygrid.com/modules/"

  var defaults = {
    resizable: true,
    editable: true
  }

  Fancy.defineTheme('dash', {
    config: {
      cellHeaderHeight: 30,
      cellHeight: 32,
      titleHeight: 42,
      barHeight: 37,
      bottomScrollHeight: 12
    }
  })

  var tabs = []

  </script>
</head>

<body>
  <div class="icon-bar">
    {{{nav_bar}}}
  </div>

  {{{body}}}

  <div id="tabs"></div>

  <script>
    window.onload = function(){
      new FancyTab({
        renderTo: 'tabs',
        title: title,
        height: 'fit',
        width:'1570',
        activeTab: 0,
        resizable: true,
        editable: true,
        theme: "blue",
        items: tabs
      })
    }
  </script>
</body>
</html>
