BINRM="/bin/rm -R"
$BINRM ~/.gntrc
$BINRM ~/.gitconfig
$BINRM ~/.config/user-dirs.conf
$BINRM ~/.tmux.conf
$BINRM ~/menv
$BINRM ~/.bashrc
$BINRM ~/.vimrc
$BINRM ~/.vim
$BINRM ~/.bash_profile
$BINRM ~/.bash_logout
$BINRM ~/.gdbinit
$BINRM ~/.quiltrc
$BINRM ~/.ssh
$BINRM ~/.config/user-dirs.conf
$BINRM ~/.config/user-dirs.dirs
$BINRM ~/.local/share/applications/gnome-start.desktop
$BINRM ~/.local/share/applications/mimeapps.desktop
$BINRM ~/.local/share/applications/root-wireshark.desktop

